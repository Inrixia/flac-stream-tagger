  "description": "Pure JavaScript FLAC Tag writer and reader.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/Inrixia/flac-stream-tagger.git"
